#include<iostream>
#include<algorithm>
#include<vector>
using namespace std;

int main() {

	vector<int> v;
	v.push_back(10);
	v.push_back(130);
	v.push_back(5);
	v.push_back(40);
	v.push_back(10);
	v.push_back(10);
	v.push_back(20);
	v.push_back(5);
	v.push_back(180);
	cout << "Vector data" << endl;
	for (int c : v)
		cout << c << ' ';
	cout << endl;

	sort(v.begin(), v.end());
	for (int c : v)
		cout << c << ' ';
	cout << endl;

	return 0;

}
